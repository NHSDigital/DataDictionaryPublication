               <row>
                  <entry>
                     <p>E02</p>
                  </entry>
                  <entry>
                     <xref outputclass="class" keyref="class_ward">WARD</xref>
                  </entry>
                  <entry>
                     <p> </p>
                  </entry>
               </row>
               <row>
                  <entry>
                     <p>E03</p>
                  </entry>
                  <entry>
                     <xref outputclass="businessDefinition"
                           keyref="business_definition_day_care_facility">Day Hospital</xref>
                  </entry>
                  <entry>
